---
import SidebarLayout from '../layouts/SidebarLayout.astro';
import DailyEntryComponent from "../components/react/DailyEntryComponent";

import { type DailyEntry } from '../types/DailyEntry';
import { type Tag } from '../types/Tag';

const entry: DailyEntry = {
    date: new Date(),
    attention: '',
    emotions: '',
    physicality: '',
    mantra: '',
    grateful: '',
    entry: '',
    tags: new Array<Tag>()
}
---

<SidebarLayout>
    <DailyEntryComponent dailyEntry={entry} client:load/>
</SidebarLayout>